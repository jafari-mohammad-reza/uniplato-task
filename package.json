  "main": "index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "seed": "pnpx prisma generate && ts-node ./src/db/seed.ts",
    "start:prod": "pnpm run seed && node dist/index.js | pino-pretty --colorize",
    "start:dev": "pnpm run seed && ts-node-dev ./src/index.ts | pino-pretty --colorize",
    "prisma": "dotenv -e .env prisma",
    "db:gen": "pnpx prisma generate",
    "db:studio": "pnpx prisma studio",
    "db:migrate": "pnpx prisma migrate dev",
    "db:push": "pnpx prisma db push",
    "lint": "./node_modules/.bin/eslint",
    "test": "cross-env mocha -r ts-node/register 'test/**/*.ts'"
  },
  "keywords": [],
