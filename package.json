    "@fastify/jwt": "^8.0.0",
    "@fastify/swagger": "^8.13.0",
    "@fastify/swagger-ui": "^2.0.1",
    "@prisma/client": "^5.8.0",
    "dotenv": "^16.3.1",
    "fastify": "^4.25.2",
    "fluent-json-schema": "^4.2.1",
    "jsonwebtoken": "^9.0.2",
    "pino": "^8.17.2",
